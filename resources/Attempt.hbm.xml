<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kydlai.web33.Attempt" table="attempt">
        <id name="id" type="java.lang.Long" column="id">
            <generator class="increment"></generator>
        </id>
        <property name="hit" type="java.lang.Boolean">
            <column name="hit"></column>
        </property>
        <property column="start_time" name="startTime" type="java.lang.String"/>
        <property column="x" name="x" type="java.lang.Double"/>
        <property column="y" name="y" type="java.lang.Double"/>
        <property column="r" name="r" type="java.lang.Double"/>
    </class>
</hibernate-mapping>